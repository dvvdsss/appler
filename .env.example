# 服务器配置
PORT=3001
NODE_ENV=production

# 数据库配置
EMBEDDING_DB_HOST=localhost
EMBEDDING_DB_PORT=5432
EMBEDDING_DB_DATABASE=apple_rag_db
EMBEDDING_DB_USER=apple_rag_user
EMBEDDING_DB_PASSWORD=your_password
EMBEDDING_DB_SSLMODE=disable

# 会话配置
SESSION_SECRET=your_secure_session_secret
SESSION_TIMEOUT=1800

# 搜索配置
USE_HYBRID_SEARCH=true

# Cloudflare D1 配置（用于用户认证）
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_D1_DATABASE_ID=your_d1_database_id

# SiliconFlow API 配置（用于向量嵌入）
SILICONFLOW_API_KEY=your_siliconflow_api_key
SILICONFLOW_TIMEOUT=30
